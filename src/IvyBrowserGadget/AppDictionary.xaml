<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="InvalidCommandlineOption_Title">IvyBrowserGadget</system:String>
    <system:String x:Key="InvalidCommandlineOption">Invalid commandline arguments</system:String>

    <system:String x:Key="SettingSaveAsDialog_Title">Save setting file as...</system:String>
    <system:String x:Key="SettingSaveAsDialog_Filter">setting file(*.xml)|*.xml</system:String>


    <system:String x:Key="MessageBoxInitSetting_Title">Initialize Setting</system:String>
    <system:String x:Key="MessageBoxInitSetting_Text">Cannot find the configuration file. Do you want to initialize the settings?</system:String>

    <system:String x:Key="InputTextCreateShortcut_Title">Create shortcut file</system:String>
    <system:String x:Key="InputTextCreateShortcut_Label" xml:space="preserve">Enter a file name for the shortcut file to be created on the desktop.&#13;Setting file: </system:String>

    <system:String x:Key="MessageBoxCreateShortcutOverwrite_Text" xml:space="preserve">Shortcut file already exists. Overwrite it?</system:String>
    <system:String x:Key="MessageBoxCreateShortcutOverwrite_Title" xml:space="preserve">Save shortcut link file</system:String>


</ResourceDictionary>
    